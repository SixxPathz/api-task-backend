{
  "version": 2,
  "builds": [
    {
      "src": "build-maven.sh",
      "use": "@vercel/static-build",
      "config": {
        "outputDirectory": "target",
        "buildCommand": "bash build-maven.sh"
      }
    },
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "index.js"
    }
  ]
}
