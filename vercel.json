{
  "version": 2,
  "builds": [
    {
      "src": "pom.xml",
      "use": "@vercel/static-build",
      "config": {
        "outputDirectory": "target",
        "buildCommand": "mvn clean install"
      }
    },
    {
      "src": "api/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "api/index.js"
    }
  ]
}
